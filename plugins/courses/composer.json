{
    "name": "huyle/courses",
    "type": "library",
    "license": "MIT",
    "autoload": {
      "psr-4": {
            "Huyle\\Courses\\": "src/",
            "Huyle\\Courses\\Providers\\":"src/Providers/",
            "Huyle\\Courses\\Database\\Migrations\\": "database/migrations/",
            "Huyle\\Courses\\Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "src/Controllers",
            "src/Providers",
            "src/Models",
            "./routes"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huyle\\Courses\\Providers\\CoursesServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan migrate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
    
}
