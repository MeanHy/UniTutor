{
    "name": "huyle/pay",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Huyle\\Pay\\": "src/",
            "Huyle\\Pay\\Providers\\": "src/Providers/",
            "Huyle\\Pay\\Database\\Migrations\\": "database/migrations/",
            "Huyle\\Pay\\Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "src/Controllers",
            "src/Providers",
            "src/Models",
            "./routes"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huyle\\Pay\\Providers\\PayServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan migrate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}