{
    "name": "laravel/project",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.2",
        "huyle/user": "*",
        "huyle/profile": "*",
        "huyle/courses": "*",
        "huyle/bookings": "*",
        "huyle/wishlist": "*",
        "huyle/pay": "*",
        "inertiajs/inertia-laravel": "^1.3",
        "laravel/framework": "^11.00",
        "laravel/sanctum": "*",
        "laravel/tinker": "*",
        "maatwebsite/excel": "*",
        "mnsami/composer-custom-directory-installer": "*",
        "spatie/laravel-permission": "^6.7",
        "tymon/jwt-auth": "^2.1",
        "wikimedia/composer-merge-plugin": "*"
    },
    "require-dev": {
        "fakerphp/faker": "*",
        "laravel/pint": "*",
        "laravel/sail": "*",
        "mockery/mockery": "*",
        "nunomaduro/collision": "*",
        "phpunit/phpunit": "*",
        "spatie/laravel-ignition": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@/usr/local/bin/php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "plugins/*/composer.json",
                "plugins/*/plugins/composer.json",
                "plugins/*/plugins/*/composer.json",
                "plugins/*/plugins/*/*/composer.json",
                "plugins/core/plugins/tightenco/ziggy/composer.json"
            ],
            "recurse": true,
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        },
        "installer-paths": {
            "./plugins/user": [
                "huyle/user"
            ],
            "./plugins/profile": [
                "huyle/profile"
            ],
            "./plugins/courses": [
                "huyle/courses"
            ],
            "./plugins/bookings": [
                "huyle/bookings"
            ],
            "./plugins/wishlist": [
                "huyle/wishlist"
            ],
            "./plugins/pay": [
                "huyle/pay"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "mnsami/composer-custom-directory-installer": true,
            "wikimedia/composer-merge-plugin": true,
            "codezero/composer-preload-files": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./packages/user/"
        },
        {
            "type": "path",
            "url": "./packages/profile/"
        },
        {
            "type": "path",
            "url": "./packages/courses/"
        },
        {
            "type": "path",
            "url": "./packages/bookings/"
        },
        {
            "type": "path",
            "url": "./packages/wishlist/"
        },
        {
            "type": "path",
            "url": "./packages/pay/"
        }
    ]
}